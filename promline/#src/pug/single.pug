mixin hsl-item(title, desc)
  .hose-sys__list-item
    .hose-sys__item-title!=title
    .hose-sys__item-desc!=desc

mixin tl-item(img, desc)
  .technology-list__item
    .technology-list__item-img
      img(src=img, alt="")
    .technology-list__item-desc!=desc

